{
  "name": "cdk-template",
  "version": "0.2.0",
  "description": "This is a template for CDK related projects.",
  "main": "bin/index.ts",
  "repository": "https://github.com/bmacher/cdk-template",
  "author": "Benjamin Macher",
  "license": "MIT",
  "scripts": {
    "postinstall": "ts-node scripts/npm-post-install-hook.ts",
    "cdk": "cdk",
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "yarn build && jest",
    "test:verbose": "yarn build && jest --verbose",
    "test:coverage": "yarn test:clean && yarn build && jest --coverage",
    "test:clean": "rm -rf coverage"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.85.0",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.10.1",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.32.0",
    "aws-cdk": "1.85.0",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.4.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "1.85.0",
    "@aws-cdk/core": "1.85.0",
    "source-map-support": "^0.5.20"
  }
}
